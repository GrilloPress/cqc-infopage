<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <%= stylesheet_link_tag "main" %>
    <%= javascript_include_tag  "main" %>
  </head>
  
  <body class="container-fluid">
    
    <div class="row">
      <div class="col-md-10 col-md-offset-1">
        <header id="cqc">
          <%= image_tag "banner.png", class: "img-responsive", width: "100%" %>
          <div class="jumbotron">
            <h1>We are all Proud to Make a Difference to our patients and the CQC visit is an opportunity to showcase our hard work.</h1>
          </div>
        </header>
      </div>
    </div>
      
    
    <div class="row">
      <div class="col-md-10 col-md-offset-1">
        
        <div class="row">
          <div class="col-md-3 col-sm-3">
            <aside id="sidebar">
              <%= partial "partials/sidebar" %>
            </aside>
          </div>
          
          <div class="col-md-8 col-sm-8">
            <main id="<%= page_classes %>">
              <%= yield %>
            </main>
          </div>
        </div>
      </div>
    </div>
    
    <div class="col-md-10 col-md-offset-1">
      <footer id="cqc">
      
      </footer>
    </div>
  </body>
</html>